fun fib(n) {
    if (n <= 1) return n;
    return fib(n - 1) + fib(n - 0x2);
}

print fib;
for (var i = 0; i < 21; i = i + 1)
    print "fib" + ' ' + (i+1) + " : " + fib(i);


